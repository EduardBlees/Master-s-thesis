@model WebPaperAnalyzer.ViewModels.DictionaryModel

@{
    ViewData["Title"] = "AddDictionary";
}

<h2>Загрузить словарь</h2>
<form method="post" enctype="multipart/form-data" asp-controller="StudentTeacher" asp-action="AddDictionary">
    <div class="form-group">
        <div class="col-md-10">
            <input type="text" asp-for="Name" />
        </div>
        <div class="col-md-10">
            <p>Upload one or more files using this form:</p><br />
            <input type="file" asp-for="File" id="UploadFile"/><br /><br />
            <label for="UploadFile">Выберите файл</label>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Save" />
        </div>
    </div>
</form>
<p>Версия v.4</p>


